<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE config>
<config protocol="Rs-422" name="">
    <param description="Скорость передачи данных" type="enumeration" value="0" name="baudRate">
        <elem>9600</elem>
        <elem>19200</elem>
    </param>
    <param description="Контроль четности" type="boolean" value="1" name="fParity"/>
    <param description="Схема контроля четности" type="enumeration" value="2" name="parity">
        <elem>EVENPARITY</elem>
        <elem>MARKPARITY</elem>
        <elem>NOPARITY</elem>
        <elem>ODDPARITY</elem>
        <elem>SPACEPARITY</elem>
    </param>
    <param description="Двоичный режим обмена" type="boolean" value="0" name="fBinary"/>
    <param description="Число информационных бит" type="integer" value="6" min="4" name="byteSize" max="8"/>
</config>
